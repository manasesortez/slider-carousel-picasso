<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.amtodev.slider_carousel_android">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_movies"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_movies"
        android:supportsRtl="true"
        android:theme="@style/Theme.Slidercarouselandroid">
        <activity
            android:name=".splash_screen"
            android:exported="true"
            android:theme="@style/bitcoinOne">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:theme="@style/bitcoinTwo"
            >

        </activity>
    </application>

</manifest>